      "compiler": "12.0",
      "platforms": "Win32, Win64",
      "template": "12+"
    }
  ],
  "templates": [
    {
      "name": "default",
      "dependencies" : [
       {
         "id" : "VSoft.CancellationToken",
         "version" : "[0.1.2,]"
       },
       {
         "id" : "Gabr42.OmniThreadLibrary",
         "version" : "[3.7.9,]"
       }
      ],
      "source": [
        {
          "src": "source\\*.pas",
          "flatten": true,
          "dest": "source"
        },
        {
          "src": "packages\\Rad Studio $compilerWithCodeName$\\*.dpk",
          "dest": "packages\\Rad Studio $compilerWithCodeName$"
        },
        {
          "src": "packages\\Rad Studio $compilerWithCodeName$\\*.dproj",
          "dest": "packages\\Rad Studio $compilerWithCodeName$"
        }
      ],
      "searchPaths": [
        {
          "path": "source"
        }
      ],
      "build": [
        {
          "id": "Runtime",
          "project": ".\\packages\\Rad Studio $compilerWithCodeName$\\VSoft.AwaitableR.dproj"
        }
      ]
    },
    {
      "name": "12+",
      "dependencies" : [
       {
         "id" : "VSoft.CancellationToken",
         "version" : "[0.1.2,]"
       },
       {
         "id" : "Gabr42.OmniThreadLibrary",
         "version" : "[3.7.9,]"
       }
      ],
      "source": [
        {
          "src": "source\\*.pas",
          "flatten": true,
          "dest": "source"
        },
        {
          "src": "packages\\Rad Studio $compilerNoPoint$\\*.dpk",
